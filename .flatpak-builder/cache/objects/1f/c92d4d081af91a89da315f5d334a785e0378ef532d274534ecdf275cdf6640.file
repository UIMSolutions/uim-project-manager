[Application]
name=io.github.UIMSolutions.UIMProjectManager
runtime=org.gnome.Platform/x86_64/46
sdk=org.gnome.Sdk/x86_64/46
command=uim-project-manager

[Context]
shared=ipc;
sockets=x11;wayland;fallback-x11;
devices=dri;
filesystems=xdg-documents;

[Extension io.github.UIMSolutions.UIMProjectManager.Debug]
directory=lib/debug
autodelete=true
no-autodownload=true

[Build]
built-extensions=io.github.UIMSolutions.UIMProjectManager.Debug;